global
    log /dev/log local0
    daemon
    maxconn 1024

defaults
    log     global
    mode    http
    timeout connect 5000ms
    timeout client  50000ms
    timeout server  50000ms

frontend http_front
    bind *:8990
    default_backend http_back

backend http_back
    balance roundrobin
    server web1 127.0.0.1:8990 check